[Unit]
Description=yorstory
After=network.target
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=always
RestartSec=1
User=root
WorkingDirectory=/usr/bin/yorstory
ExecStart=/usr/bin/yorstory/yorstory static.bigdata data/empty.bigdata data/portfolio.json 443 /etc/letsencrypt/live/yorstory.ca/fullchain.pem /etc/letsencrypt/live/yorstory.ca/privkey.pem

[Install]
WantedBy=multi-user.target